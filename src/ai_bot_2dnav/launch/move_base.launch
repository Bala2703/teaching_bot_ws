<launch>

    <master auto="start"/>
  <!-- Run the map server --> 
  <arg name="map_file" default="$(find ai_bot_2dnav)/maps/with_imu.yaml"/>

  <!-- Run the map server -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" /> 
  <!--- Run AMCL --> 
 
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
     <rosparam file="$(find ai_bot_2dnav)/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
     <rosparam file="$(find ai_bot_2dnav)/costmap_common_params.yaml" command="load" ns="local_costmap" />
     <rosparam file="$(find ai_bot_2dnav)/local_costmap_params.yaml" command="load" />
     <rosparam file="$(find ai_bot_2dnav)/global_costmap_params.yaml" command="load" /> 
     <rosparam file="$(find ai_bot_2dnav)/base_local_planner_params.yaml" command="load" />
  </node>
 
 </launch> 