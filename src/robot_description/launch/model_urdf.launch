  <launch>


    <node name="rplidarNode"          pkg="rplidar_ros"  type="rplidarNode" output="screen">
    <param name="serial_port"         type="string" value="/dev/ttyUSB1"/>
    <param name="serial_baudrate"     type="int"    value="256000"/>
    <param name="frame_id"            type="string" value="laser"/>
    <param name="inverted"            type="bool"   value="false"/>
    <param name="angle_compensate"    type="bool"   value="true"/>
    <remap from="/scan" to="/base_scan"/>
    </node>

      <!-- <node name="ros_bno08x" pkg="ros_bno08x" type="talker.py" respawn="true" respawn_delay="2">
      </node> -->

      <param name="robot_description"
          command="$(find xacro)/xacro '$(find robot_description)/urdf/include/ai_bot.xacro'" />


      <node name="joint_state_publisher"
          pkg="joint_state_publisher"
          type="joint_state_publisher" />
      <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" >
      </node>

  <!--    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find robot_description)/urdf/robot.rviz"
          required="true" /> -->
  </launch>
